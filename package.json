{
  "name": "ianai-cli",
  "version": "1.1.2",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "bin": {
    "ai": "./dist/main.mjs"
  },
  "files": [
    "dist"
  ],
  "repository": "github:cyb1999/ianai-cli.git",
  "scripts": {
    "copy-schema-files": "cp src/*-schema.ts dist/",
    "build": "rm -rf dist/ && pkgroll --minify && pnpm copy-schema-file",
    "build:watch": "pkgroll --watch",
    "start": "jiti ./dist/main.mjs echo Hello World!",
    "dev": "jiti ./src/main.ts",
    "dev:init": "jiti ./src/main.ts --init",
    "dev:version": "jiti ./src/main.ts --version",
    "release:patch": "pnpm version patch && pnpm build && npm publish && git push --follow-tags"
  },
  "author": "Ian <https://github.com/cyb1999>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.14.10",
    "jiti": "^1.21.6",
    "pkgroll": "^2.1.1",
    "typescript": "^5.5.3"
  },
  "keywords": [
    "ai",
    "cli",
    "command line tool",
    "deepseek"
  ],
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "chalk": "^5.3.0",
    "cleye": "^1.3.2",
    "clipboardy": "^2.3.0",
    "commander": "^12.1.0",
    "es-toolkit": "^1.12.0",
    "execa": "^9.3.0",
    "zod": "^3.23.8"
  }
}
